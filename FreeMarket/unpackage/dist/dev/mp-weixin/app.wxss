.content {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.report {
  width: 15vw;
  height: 5vh;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-items: center;
  justify-content: space-evenly;
}
.searchBox {
  width: 52vw;
  height: 5vh;
  background-color: white;
  margin-left: 2vw;
  border: #c8c7cc 1px solid;
  border-radius: 2.5vh;
  line-height: 5vh;
  display: flex;
  flex-direction: row-reverse;
}
.searchBox #searchBtn {
  text-align: center;
  width: 18vw;
  background-color: #007aff;
  color: white;
  border-radius: 2.5vh;
}
.searchBox text {
  margin-right: 21vw;
  color: #c8c7cc;
